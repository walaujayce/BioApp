    "/api/near_data_bird": {
      "get": {
        "tags": [
          "user_bird"
        ],
        "summary": "取得目前最近鳥類資料",
        "description": "取得目前最近鳥類資料，st代表開始位置，從0開始計算，回傳num筆，如果使用者上傳編號未到st+num筆則只回傳到使用者目前最大筆數，lon代表使用者經度，lat代表使用者緯度。",
        "operationId": "get_near_data_bird",
        "parameters": [
          {
            "name": "st",
            "in": "query",
            "description": "資料順序開始位置",
            "required": true,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer",
              "default": 0
            }
          },
          {
            "name": "num",
            "in": "query",
            "description": "取得資料數量",
            "required": true,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer",
              "default": 10
            }
          },
          {
            "name": "lat",
            "in": "query",
            "description": "以某緯度做最近資料搜查，需要和lon一起填寫",
            "required": true,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "lon",
            "in": "query",
            "description": "以某經度做最近資料搜查，需要和lat一起填寫",
            "required": true,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "id",
            "in": "query",
            "description": "指定某個id的資料",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "sort",
            "in": "query",
            "description": "以某項參數做排序,可接授\"id\", \"name\", \"description\", \"lon\", \"lat\", \"time\"",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "default": "id"
            }
          },
          {
            "name": "order",
            "in": "query",
            "description": "DESC或是ASC",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "default": "DESC"
            }
          },
          {
            "name": "filter",
            "in": "query",
            "description": "關鍵字搜尋",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "upload",
            "in": "query",
            "description": "是否上鏈",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "boolean",
              "default": false
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/inline_response_200_1"
                }
              }
            }
          },
          "400": {
            "description": "wrong request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/response_ERROR"
                }
              }
            }
          }
        },
        "security": [
          {
            "ApiKeyAuth": []
          }
        ]
